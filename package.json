{
  "name": "voluntree",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "start:client": "cross-env PORT=3000 npm run start --prefix client",
    "start:server": "npm run start --prefix server",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\""
  }
}
